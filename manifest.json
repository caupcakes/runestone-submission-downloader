{
  "manifest_version": 3,
  "name": "Runestone Submission Downloader",
  "version": "6.9.420",
  "host_permissions": [
    "*://runestone.academy/"
  ],
  "permissions": [
    "cookies",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://runestone.academy/runestone/admin/grading"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}